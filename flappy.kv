FloatLayout:
    Background:
        id: background
        canvas.before:
            Rectangle:
                size: self.size
                pos: self.pos
                source: "flappy-jay-background.png"

            Rectangle:
                size: self.width, 138
                pos: self.pos[0], self.pos[1] + self.height - 138
                texture: self.cloud_texture
        



